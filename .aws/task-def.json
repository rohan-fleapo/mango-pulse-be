{
  "family": "mango-pulse-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "mango-pulse-container",
      "image": "my-app-image",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/mango-pulse-task",
          "awslogs-region": "ap-south-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::599076352496:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::599076352496:role/ecsTaskRole"
}
