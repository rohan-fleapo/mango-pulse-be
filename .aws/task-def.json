{
  "family": "mango-pulse-repo-task",
  "containerDefinitions": [
    {
      "name": "mango-pulse-repo",
      "image": "599076352496.dkr.ecr.ap-south-2.amazonaws.com/mango-pulse-repo:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "8000",
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-south-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::599076352496:role/ecsTaskRole",
  "executionRoleArn": "arn:aws:iam::599076352496:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
